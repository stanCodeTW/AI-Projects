 elif actual_price >= max_predicted_price:  # 賣出\n","          trades.append({'date': date, 'action': 'buy', 'price': actual_price})\n","          trade_log.append({'date': date, 'action': 'buy', 'price': actual_price, 'close_date':'', 'return':0})\n","\n","    # 平倉條件\n","    for trade in trades:\n","        trade_date = trade['date']  # 交易時間\n","        log_return = np.log(actual_price / trade['price'])  # 這天的log return(跟成本價格相比)\n","        actual_return = trade['price'] - actual_price\n","        if trade['action'] == 'buy':  # 買入價格大於下限or達到停損(利)點10%\n","            if trade['price'] <= max_predicted_price or log_return >= SENTINEL*2 or log_return <= -SENTINEL/2:\n","                trade_log.append({'date': trade_date, 'action': 'close_and_sell', 'price': 0, 'close_date':date, 'return':actual_return})\n","                trades.remove(trade)  # 移除已平倉的交易\n","\n","        elif trade['action'] == 'sell':  # 賣出價格小於上限or達到停損(利)點10%\n","            if trade['price'] >= min_predicted_price or log_return >= SENTINEL*2 or log_return <= -SENTINEL/2:\n","                trade_log.append({'date': trade_date, 'action': 'close_and_buy', 'price': 0, 'close_date':date, 'return':actual_return})\n","                trades.remove(trade)  # 移除已平倉的交易\n","\n","# 將交易記錄轉換為數據框\n","trade_log_df = pd.DataFrame(trade_log)\n","print(trade_log_df)\n","\n","\n","# 假設您已經有一個名為 df 的 DataFrame\n","df = trade_log_df.copy()\n","\n","# 進行排序，按照 'date' 列和 'action' 列（如果需要的話）\n","sorted_df = df.sort_values(by=['date', 'action'], ascending=[True, True])\n","\n","# 重新設定索引（可選）\n","sorted_df.reset_index(drop=True, inplace=True)\n","\n","# 顯示排序後的 DataFrame\n","print(sorted_df)\n","\n","total_return = sorted_df['return'].sum()\n","print(f\"Total Return: {total_return:.6f}\")\n"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"CxI2QgB7N2nq","executionInfo":{"status":"ok","timestamp":1727444740379,"user_tz":-480,"elapsed":771,"user":{"displayName":"sc201jun24c","userId":"02069774851930120564"}},"o