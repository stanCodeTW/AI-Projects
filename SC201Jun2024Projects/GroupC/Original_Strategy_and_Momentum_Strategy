"df_prices.index = pd.to_datetime(df_prices.index)\n","df_trades.loc[:, 'date'] = pd.to_datetime(df_trades['date'])\n","df_trades.loc[:, 'close_date'] = pd.to_datetime(df_trades['close_date'])\n","\n","# 獲取第一筆交易的日期作為開始日期\n","start_date = df_trades['date'].min()\n","\n","# 查找交易的結束日期\n","matching_rows = df_trades[df_trades['date'] == start_date + pd.Timedelta(days=LENGTH)]\n","if not matching_rows.empty:\n","    end_date = matching_rows['close_date'].values[0]\n","else:\n","    # 如果沒有找到匹配的 close_date，則使用 start_date + LENGTH 天作為預設結束日期\n","    end_date = start_date + pd.Timedelta(days=LENGTH)\n","\n","# 獲取對應日期範圍的價格數據\n","df_prices_30 = df_prices[(df_prices.index >= start_date) & (df_prices.index <= end_date)]\n","\n","# 獲取在相同日期範圍內的交易資料\n","df_trades_30 = df_trades[(df_trades['date'] >= start_date) & (df_trades['close_date'] <= end_date)]\n","\n","# 設定圖形大小\n","# fig, ax1 = plt.subplots(figsize=(12, 5))\n","fig, ax1 = plt.subplots(figsize=(8, 5))\n","\n","# 畫實際價格的折線圖\n","ax1.plot(df_prices_30.index, df_prices_30['actual_price'], label='Actual Price')\n","\n","# 畫預測價格的上下界線\n","ax1.plot(df_prices_30.index, df_prices_30['min_predicted_price'], label='Lower Bound', color='darkgray')\n","ax1.plot(df_prices_30.index, df_prices_30['max_predicted_price'], label='Upper Bound', color='darkgray')\n","\n","# 標註交易點\n","for _, trade in df_trades_30.iterrows():\n","    trade_date = trade['date']\n","    close_date = trade['close_date']\n","    action = trade['action']\n","\n","    actual_close_price = df_prices_30[df_prices_30.index == close_date]['actual_price'].values[0]\n","    if action == 'buy':\n","        ax1.scatter(trade_date, trade['price'], marker='^', color='green', s=SIZE, zorder=5)\n","        ax1.scatter(close_date, actual_close_price + OFFSET, marker='v', color='#8B0000', s=SIZE, zorder=5, alpha=0.9)\n","    elif action == 'sell':\n","        ax1.scatter(trade_date, trade['price'], marker='v', color='red', s=SIZE, zorder=5)\n","        ax1.scatter(close_date, actual_close_price - OFFSET, marker='^', color='#4CAF50', s=SIZE, zorder=5, alpha=1)\n","\n","# 計算累積回報\n","cumulative_returns = []\n","current_return = 0  # 初始化回報為0\n","for i, trade in df_trades_30.iterrows():\n","    trade_return = trade['return']\n","    trade_date = trade['date']\n","    close_date = trade['close_date']\n","\n","    # 在平倉日更新回報\n","    while trade_date <= close_date:\n","        cumulative_returns.append((trade_date, current_return))\n","        trade_date += pd.Timedelta(days=1)\n","\n","    # 更新累積回報\n","    current_return += trade_return\n","    cumulative_returns.append((close_date, current_return))\n","\n","# 將累積回報轉換為 DataFrame\n","df_cumulative_returns = pd.DataFrame(cumulative_returns, columns=['date', 'cumulative_return'])\n","df_cumulative_returns.set_index('date', inplace=True)\n","\n","# 繪製累積回報的長條圖\n","colors = ['green' if val >= 0 else 'red' for val in df_cumulative_returns['cumulative_return']]\n","ax1.bar(df_cumulative_returns.index, df_cumulative_returns['cumulative_return'], color=colors, alpha=0.6)\n","\n","# 設定標題與標籤\n","ax1.set_title('Price Trend with Trading Actions (Closed Based on Log Return 10%)')\n","ax1.set_xlabel('Date')\n","ax1.set_ylabel('Price')\n","# ax1.legend(loc='upper left')\n","# 設定圖例位置為左下方並稍微向上調整\n","ax1.legend(loc='lower left', bbox_to_anchor=(0, 0.3))\n","ax1.grid()\n","\n","# 創建第二個 y 軸\n","ax2 = ax1.twinx()  # 創建新的 y 軸\n","ax2.set_ylabel('Cumulative Return')\n","\n","# 同步 y 軸範圍\n","ax2.set_ylim(ax1.get_ylim())  # 將 ax2 的範圍與 ax1 一致\n","\n","# 在 y=0 的位置繪製水平線\n","ax2.axhline(0, color='black', linewidth=0.8, linestyle='--')\n","\n","# 顯示圖形\n","plt.xticks(rotation=45)\n","plt.tight_layout()\n","plt.show()\n"],"metadata":{"id":"J8AN734UyYcH","colab":{"base_uri":"https://localhost:8080/","height":507},"executionInfo":{"status":"ok","timestamp":1727440751120,"user_tz":-480,"elapsed":1687,"user":{"displayName":"sc201jun24c","userId":"02069774851930120564"}},"outputId":"5962b958-f3db-43be-a6f0-c57560d96916"},"execution_count":null,"outputs":[{"output_type":"display_data","data":{"text/plain":["<Figure size 800x500 with 2 Axes>"],"image/png":"\n"},"metadata":{}}]},{"cell_type":"code","source":["\n","CLOSE_DAYS = 10\n","# 假設 df 是你的 DataFrame\n","df = bands_df.copy()  # 讀取資料\n","\n","# 設置初始變量\n","trade_log = []\n","open_trades = []  # 用來追蹤未平倉的交易\n","\n","# 交易策略\n","for i in range(len(df)):\n","    date_str = df.index[i]  # 獲取字符串形式的日期\n","    date = pd.to_datetime(date_str)  # 轉換為 datetime 對象\n","    actual_price = df.iloc[i]['actual_price']\n","    min_predicted_price = df.iloc[i]['min_predicted_price']\n","    max_predicted_price = df.iloc[i]['max_predicted_price']\n","\n","    # 平倉條件\n","    for trade in open_trades[:]:  # 使用[:]來複製列表，避免在迭代時修改\n","        if i >= trade['open_index'] + CLOSE_DAYS:  # 如果已經超過CLOSE_DAYS\n","            if trade['action'] == 'buy':\n","                # 計算回報：實際賣出價格 - 開倉價格\n","                actual_return = actual_price - trade['price']\n","                trade_log.append({\n","                    'date': trade['date'],\n","                    'action': 'close_and_sell',\n","                    'price': actual_price,\n","                    'close_date': date,\n","                    'return': actual_return\n","                })\n","            elif trade['action'] == 'sell':\n","                # 計算回報：開倉價格 - 實際買入價格\n","                actual_return = trade['price'] - actual_price\n","                trade_log.append({\n","                    'date': trade['date'],\n","                    'action': 'close_and_buy',\n","                    'price': actual_price,\n","                    'close_date': date,\n","                    'return': actual_return\n","                })\n","\n","            open_trades.remove(trade)  # 移除已平倉的交易\n","\n","    # 開倉條件\n","    if not open_trades:  # 只有在沒有開倉的情況下才能開倉\n","        if actual_price <= min_predicted_price:  # 買入\n","            open_trades.append({'date': date, 'action': 'buy', 'price': actual_price, 'open_index': i})\n","            trade_log.append({'date': date, 'action': 'buy', 'price': actual_price, 'close_date': '', 'return': 0})\n","\n","        elif actual_price >= max_predicted_price:  # 賣出\n","            open_trades.append({'date': date, 'action': 'sell', 'price': actual_price, 'open_index': i})\n","            trade_log.append({'date': date, 'action': 'sell', 'price': actual_price, 'close_date': '', 'return': 0})\n","\n","# 將交易記錄轉換為數據框\n","trade_log_df = pd.DataFrame(trade_log)\n","print(trade_log_df)\n","\n","# 進行排序，按照 'date' 列和 'action' 列（如果需要的話）\n","sorted_df = trade_log_df.sort_values(by=['date', 'action'], ascending=[True, True])\n","\n","# 重新設定索引（可選）\n","sorted_df.reset_index(drop=True, inplace=True)\n","\n","# 顯示排序後的 DataFrame\n","print(sorted_df)\n","\n","# 計算總回報\n","total_return = sorted_df['return'].sum()\n","print(f\"Total Return: {total_return:.6f}\")\n"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"w8z31w_RxXHe","executionInfo":{"status":"ok","timestamp":1727446306059,"user_tz":-480,"elapsed":680,"user":{"displayName":"sc201jun24c","userId":"02069774851930120564"}},"outputId":"010982fa-3f35-46cf-f7a7-d8f6a0c26e1e"},"execution_count":null,"outputs":[{"output_type":"stream","name":"stdout","text":["         date          action       price           close_date     return\n","0  2022-08-31             buy   15.076043                        0.000000\n","1  2022-08-31  close_and_sell   13.116205  2022-09-14 00:00:00  -1.959838\n","2  2022-09-14             buy   13.116205                        0.000000\n","3  2022-09-14  close_and_sell   12.724557  2022-09-28 00:00:00  -0.391648\n","4  2022-09-28             buy   12.724557                        0.000000\n","..        ...             ...         ...                  ...        ...\n","81 2024-07-15  close_and_sell  111.580657  2024-07-29 00:00:00 -16.848580\n","82 2024-07-29             buy  111.580657                        0.000000\n","83 2024-07-29  close_and_sell  109.010875  2024-08-12 