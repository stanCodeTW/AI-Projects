}\")\n","total_return = df_summary['return'].sum()\n","print(f\"Total Return(USD): {total_return:.6f}\")"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"2AV4f39_ymlw","executionInfo":{"status":"ok","timestamp":1727446323783,"user_tz":-480,"elapsed":297,"user":{"displayName":"sc201jun24c","userId":"02069774851930120564"}},"outputId":"9f8be8a0-1b40-4ba0-b365-f977e58819c1"},"execution_count":null,"outputs":[{"output_type":"stream","name":"stdout","text":["        date action      price close_date    return\n","0 2022-08-31    buy  28.192248 2022-09-14 -1.959838\n","1 2022-09-14    buy  25.840762 2022-09-28 -0.391648\n","2 2022-09-28    buy  24.214225 2022-10-12 -1.234889\n","3 2022-10-12    buy  24.374082 2022-10-26  1.394746\n","4 2022-11-08   sell  30.612473 2022-11-22 -1.434709\n","Total number of trades: 43\n","Total Return(USD): -59.943513\n"]}]},{"cell_type":"code","source":["import pandas as pd\n","import numpy as np\n","import matplotlib.pyplot as plt\n","import matplotlib.dates as mdates\n","\n","# 設置常量\n","NDAYS = 523\n","OFFSET = 0  # 讓平倉標誌不要重疊\n","TARGET_END_DATE = pd.to_datetime(\"2024-08-26\")  # 設定目標結束日期\n","SIZE = 30  # 標記的尺寸\n","\n","plt.style.use('default')\n","\n","# 只選擇前 N 天的數據\n","price_predictions_60_days = bands_df.head(NDAYS)\n","price_predictions_60_days.index = pd.to_datetime(price_predictions_60_days.index)\n","\n","# 使用你自己的 DataFrame\n","df_prices = bands_df.copy()\n","df_trades = df_summary.copy()\n","\n","# 轉換日期格式\n","df_prices.index = pd.to_datetime(df_prices.index)\n","df_trades['date'] = pd.to_datetime(df_trades['date'])\n","df_trades['close_date'] = pd.to_datetime(df_trades['close_date'])\n","\n","# 獲取第一筆交易的日期作為開始日期\n","start_date = df_trades['date'].min()\n","\n","# 將結束日期設定為指定日期\n","end_date = TARGET_END_DATE\n","\n","# 獲取對應日期範圍的價格數據\n","df_prices_30 = df_prices[(df_prices.index >= start_date) & (df_prices.index <= end_date)]\n","\n","# 獲取在相同日期範圍內的交易資料\n","df_trades_30 = df_trades[(df_trades['date'] >= start_date) & (df_trades['close_date'] <= end_date)]\n","\n","# 繪圖\n","plt.figure(figsize=(12, 4))\n","plt.plot(price_predictions_60_days.index, price_predictions_60_days['min_predicted_price'], label='Lower Bound', color='darkgray')\n","plt.plot(price_predictions_60_days.index, price_predictions_60_days['max_predicted_price'], label='Upper Bound', color='darkgray')\n","plt.plot(price_predictions_60_days.index, price_predictions_60_days['actual_price'], label='Actual Price')\n","\n","# 標註交易點\n","for _, trade in df_trades_30.iterrows():\n","    trade_date = trade['date']\n","    close_date = trade['close_date']\n","    action = trade['action']\n","\n","    # 確保交易日期對應的實際價格\n","    if trade_date in df_prices_30.index and close_date in df_prices_30.index:\n","        actual_trade_price = df_prices_30.loc[trade_date, 'actual_price']\n","        actual_close_price = df_prices_30.loc[close_date, 'actual_price']\n","\n","        if action == 'buy':\n","            plt.scatter(trade_date, actual_trade_price, marker='^', color='green', s=SIZE, zorder=5)\n","            plt.scatter(close_date, actual_close_price + OFFSET, marker='v', color='#8B0000', s=SIZE, zorder=5, alpha=0.9)\n","        elif action == 'sell':\n","            plt.scatter(trade_date, actual_trade_price, marker='v', color='red', s=SIZE, zorder=5)\n","            plt.scatter(close_date, actual_close_price - OFFSET, marker='^', color='#4CAF50', s=SIZE, zorder=5, alpha=1)\n","    else:\n","        print(f\"Missing data for trade: {trade_date} or {close_date}\")\n","\n","# 添加標題和標籤\n","plt.title(f'Price Predictions and Trading Action for Prediction Interval({NDAYS} Days)')\n","plt.xlabel('Date')\n","plt.ylabel('Price')\n","\n","# 設置 x 軸的日期格式\n","plt.gca().xaxis.set_major_formatter(mdates.DateFormatter('%Y-%m'))\n","plt.gca().xaxis.set_major_locator(mdates.MonthLocator())\n","plt.xticks(rotation=45)  # 旋轉 x 軸的標籤以便閱讀\n","plt.legend()\n","plt.grid()\n","\n","# 顯示圖形\n","plt.tight_layout()\n","plt.show()\n"],"metadata":{"colab":{"b